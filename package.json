{
  "name": "d2",
  "version": "0.0.6",
  "description": "Javascript library for DHIS2",
  "main": "d2.js",
  "scripts": {
    "validate": "gulp git:pre-commit",
    "test": "gulp test && gulp e2e",
    "prebuild": "gulp jscs &&  gulp jshint && gulp test && gulp e2e",
    "build": "./node_modules/babel/bin/babel/index.js src --out-dir dist --source-maps && cp package.json ./dist/package.json",
    "postbuild": "gulp docs:build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2.git"
  },
  "keywords": [
    "dhis2",
    "api",
    "library"
  ],
  "author": "Mark Polak",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/d2/issues"
  },
  "homepage": "https://github.com/dhis2/d2",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.1.13",
    "babel-core": "^5.5.6",
    "canonical-path": "0.0.2",
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "del": "^0.1.3",
    "dgeni": "~0.4.1",
    "dgeni-packages": "~0.10.12",
    "es6-module-loader": "^0.16.6",
    "esprima": "~2.1.0",
    "gulp": "3.8.x",
    "gulp-jscs": "^1.4.0",
    "gulp-jshint": "^1.9.0",
    "gulp-karma": "0.0.x",
    "gulp-load-plugins": "^0.8.1",
    "gulp-mocha": "^2.0.0",
    "gulp-plumber": "^0.6.6",
    "gulp-spawn-mocha": "^2.0.1",
    "jquery": "^2.1.3",
    "jscs": "^1.13.1",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.7.0",
    "jshint-stylish": "^1.0.2",
    "karma": "^0.12.31",
    "karma-babel-preprocessor": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "0.1.x",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^0.3.0",
    "karma-systemjs": "^0.5.0",
    "lodash": "~3.6.0",
    "marked": "^0.3.3",
    "mocha": "^2.1.0",
    "phantomjs": "1.9.x",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "^2.0.1",
    "proxyquire": "^1.4.0",
    "run-sequence": "^1.0.2",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0",
    "systemjs": "^0.17.1",
    "systemjs-builder": "^0.11.3"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "src/d2",
    "exportVarName": "d2",
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "registry": "jspm",
  "jspm": {
    "directories": {
      "lib": "."
    },
    "dependencies": {
      "jquery": "github:components/jquery@^2.1.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@5.5.8",
      "babel-runtime": "npm:babel-runtime@5.5.8",
      "core-js": "npm:core-js@0.9.17"
    }
  },
  "pre-commit": [
    "validate"
  ]
