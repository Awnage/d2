{
  "name": "d2",
  "version": "0.0.8",
  "description": "Javascript library for DHIS2",
  "main": "d2.js",
  "scripts": {
    "coverage": "gulp coverage",
    "validate": "gulp git:pre-commit",
    "test": "gulp test && gulp e2e",
    "test-watch": "gulp watch",
    "prebuild": "gulp jscs &&  gulp jshint && gulp test && gulp e2e",
    "build": "./node_modules/babel/bin/babel.js src --out-dir dist --source-maps && cp package.json ./dist/package.json && ./node_modules/webpack/bin/webpack.js && ./node_modules/webpack/bin/webpack.js --config webpack.config.min.js",
    "_postbuild": "gulp docs:build",
    "lint": "gulp jshint && gulp jscs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2.git"
  },
  "keywords": [
    "dhis2",
    "api",
    "library"
  ],
  "author": "Mark Polak",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/d2/issues"
  },
  "homepage": "https://github.com/dhis2/d2",
  "dependencies": {},
  "devDependencies": {
    "babel": "5.8.21",
    "babel-core": "5.8.22",
    "babel-loader": "5.3.2",
    "canonical-path": "0.0.2",
    "chai": "2.3.0",
    "coveralls": "2.11.4",
    "del": "1.2.1",
    "dgeni": "0.4.1",
    "dgeni-packages": "0.10.12",
    "es6-module-loader": "0.17.6",
    "esprima": "2.5.0",
    "gulp": "3.9.0",
    "gulp-jscs": "1.4.0",
    "gulp-jshint": "1.11.2",
    "gulp-karma": "0.0.x",
    "gulp-load-plugins": "1.0.0-rc.1",
    "gulp-mocha": "2.1.3",
    "gulp-plumber": "1.0.1",
    "gulp-spawn-mocha": "2.2.1",
    "jquery": "2.1.4",
    "jscs": "1.13.1",
    "jscs-stylish": "0.3.1",
    "jshint": "2.8.0",
    "jshint-stylish": "2.0.1",
    "karma": "0.13.9",
    "karma-babel-preprocessor": "5.2.1",
    "karma-chai": "0.1.0",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "0.3.2",
    "karma-systemjs": "0.7.2",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "mocha": "2.2.5",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "proxyquire": "1.7.1",
    "run-sequence": "1.1.2",
    "sinon": "1.16.1",
    "sinon-chai": "2.8.0",
    "systemjs": "0.18.16",
    "systemjs-builder": "0.13.4",
    "webpack": "1.12.0"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "src/d2",
    "exportVarName": "d2",
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "jspm": {
    "directories": {
      "lib": "."
    },
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@5.5.8",
      "babel-runtime": "npm:babel-runtime@5.5.8",
      "core-js": "npm:core-js@0.9.17"
    }
  },
  "pre-commit": [
    "validate"
  ]
